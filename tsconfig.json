{
  "compilerOptions": {
    "outDir": "./docs/js/",
    "lib": ["dom"],
    "noImplicitAny": false,
    "module": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}